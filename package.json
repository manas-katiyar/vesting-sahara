  "dependencies": {
    "@openzeppelin/contracts": "^4.4.2",
    "@openzeppelin/contracts-upgradeable": "^4.4.2",
    "@truffle/hdwallet-provider": "^1.7.0",
    "chance": "^1.1.8",
    "dotenv": "^10.0.0",
    "solc": "^0.8.12",
