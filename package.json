{
  "name": "sahara-vesting",
  "version": "1.0.0",
  "description": "",
  "main": "truffle-config.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "compile": "rm -rf build && npx truffle compile",
    "migrate:development": "rm -rf build && npx truffle migrate --reset --network development --f 1 --to 1",
    "migrate:dashboard": "rm -rf build && npx truffle migrate --network dashboard --f 1 --to 1",
    "migrate:mumbai": "rm -rf build && npx truffle migrate --network mumbai --f 1 --to 1",
    "test": "rm -rf build && npx truffle compile && npx truffle test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/superhow/sahara-vesting.git"
  },
